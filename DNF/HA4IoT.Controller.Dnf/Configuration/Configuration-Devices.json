{
  "DeviceRegistryService": {
    "Devices": {
      // Input(88) SCL - SCL - SCL
      "HSPE16_88": {
        "Driver": {
          "Type": "CCTools.HSPE16_InputOnly",
          "Parameters": {
            "Address": 88,
            "Interrupt": "Default",
            "PollDurationWarningThreshold": 25
          }
        }
      },
      // Input(16) GND - SCL - GND
      "HSPE16_16": {
        "Driver": {
          "Type": "CCTools.HSPE16_InputOnly",
          "Parameters": {
            "Address": 16,
            "Interrupt": "Default",
            "PollDurationWarningThreshold": 25
          }
        }
      },

      // Bridge
      "I2CHardwareBridge": {
        "Driver": {
          "Type": "I2CHardwareBridge",
          "Parameters": {
            "Address": 50
          }
        }
      },

      "RemoteSocketBridge": {
        "Driver": {
          "Type": "I2CLdp433MhzBridge",
          "Parameters": {
            "Pin": "7"
          }
        }
      },

      // Rel(32) GND - GND - GND
      "HSRel8_32": {
        "Driver": {
          "Type": "CCTools.HSRel8",
          "Parameters": {
            "Address": 32
          }
        }
      },
      // Rel(24) SCL - SCL - GND
      "HSRel8_24": {
        "Driver": {
          "Type": "CCTools.HSRel8",
          "Parameters": {
            "Address": 24
          }
        }
      }
    }
  }
}